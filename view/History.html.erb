<div id="titles">
  <h2>過去ログ画面</h2>
</div>
<div id="mainlink">
  <a href="<%= mainUrl %>">メイン画面へ</a>
</div>
<div id="roomarea">
  <form method="post" action="<%= formaction %>">
    <select name="selectroom" id="selectroom">
      <%= roomSel %>
    </select>
    <input type="hidden" name="prevroom" id="prevroom" value="<%= rprev %>">
    <input type="password" name="roompass" id="roompass" value="">
    <input type="submit" name="roomlogsearch" id="roomlogsearch" value="検索">
  </form>
  <%= iserr unless iserr.nil? %>
</div>
<% if !(strErr.nil?) %>
<div id="errarea">
  <%= strErr %>
</div>
<% elsif !(rid.empty?) %>
<div id="inputarea">
  <div id="prevlink">
  <% if existPrev %>
  <a href="<%= prevUrl %>">＜＜前の<%= splitCount %>件</a>
  <% else %>
  ＜＜前の<%= splitCount %>件
  <% end %>
  </div><div id="nextlink">
  <% if existNext %>
  <a href="<%= nextUrl %>">次の<%= splitCount %>件＞＞</a>
  <% else %>
  次の<%= splitCount %>件＞＞
  <% end %>
  </div>
</div>
<div id="logarea">
  <%= logStr %>
</div>
<% end %>
